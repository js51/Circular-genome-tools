from cgt import *
from sage.all_cmdline import *  # import sage library

gap.LoadPackage('"repsn"')

G = gap.WreathProduct(SymmetricGroup(2), SymmetricGroup(5))
IRR_CHARS = gap.Irr(G)

irrep = gap.IrreducibleAffordingRepresentation(IRR_CHARS[7])
image = gap.Image(irrep, '(1,3)(2,4)')
show(image)
show(matrix(CC, image))